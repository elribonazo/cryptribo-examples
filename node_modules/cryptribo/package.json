{
	"name": "cryptribo",
	"version": "0.1.3",
	"description": "A ready to use library including the emurgo wasm for Cardano and a set of utilities to encrypt, decrypt, sign and verify anything using native wasm",
	"main": "src/index.ts",
	"exports": {
		"./node": {
			"require": "./node/cjs/index.js",
			"import": "./node/es/index.js"
		},
		"./browser": {
			"require": "./browser/cjs/index.js",
			"import": "./browser/es/index.js"
		}
	},
	"files": [
		"src",
		"node",
		"browser",
		"index.d.ts"
	],
	"types": "index.d.ts",
	"scripts": {
		"build": "npx npm-run-all build:*",
		"build:types": "npx tsc --emitDeclarationOnly true",
		"build:node": "rm -rf node/cjs && npx rollup -c config/node/rollup.cjs.js",
		"build:node-esm": "rm -rf node/es && npx rollup -c config/node/rollup.es.js",
		"build:browser": "rm -rf browser/cjs && npx rollup -c config/browser/rollup.cjs.js",
		"build:browser-esm": "rm -rf browser/es && npx rollup -c config/browser/rollup.es.js",
		"build:cdn": "npx webpack",
		"download-deps": "git submodule update --init --recursive"
	},
	"devDependencies": {
		"@babel/core": "^7.7.2",
		"@babel/plugin-proposal-decorators": "^7.15.4",
		"@babel/plugin-proposal-export-default-from": "^7.14.5",
		"@babel/plugin-transform-runtime": "^7.15.0",
		"@babel/plugin-transform-typescript": "^7.15.4",
		"@babel/preset-env": "^7.7.1",
		"@babel/preset-typescript": "^7.15.0",
		"@babel/runtime-corejs3": "^7.15.4",
		"@emurgo/cardano-serialization-lib-browser": "^9.0.0",
		"@emurgo/cardano-serialization-lib-nodejs": "^9.0.0",
		"@rollup/plugin-commonjs": "^20.0.0",
		"@rollup/plugin-node-resolve": "^13.0.5",
		"@rollup/plugin-replace": "^3.0.0",
		"@rollup/plugin-wasm": "^5.1.2",
		"arraybuffer-loader": "^1.0.8",
		"babel-loader": "^8.2.2",
		"babel-plugin-add-module-exports": "^1.0.4",
		"babel-plugin-nameof-js": "^0.0.2",
		"bip39": "^3.0.4",
		"cardano-crypto.js": "^6.0.0",
		"cbor": "^8.0.0",
		"clean-webpack-plugin": "^4.0.0",
		"crypto": "^1.0.1",
		"crypto-browserify": "^3.12.0",
		"file-loader": "^6.2.0",
		"npm-run-all": "^4.1.5",
		"path-browserify": "^1.0.1",
		"pbkdf2": "^3.1.2",
		"process": "^0.11.10",
		"puppeteer": "^1.13.0",
		"rollup": "^2.38",
		"rollup-plugin-babel": "^4.4.0",
		"rollup-plugin-cleanup": "^3.2.1",
		"rollup-plugin-copy": "^3.4.0",
		"rollup-plugin-dts": "^4.0.0",
		"rollup-plugin-terser": "^7.0.2",
		"rollup-plugin-typescript2": "^0.19.3",
		"run-all": "^1.0.1",
		"stream-browserify": "^3.0.0",
		"typescript": "^4.1.5",
		"util": "^0.12.4",
		"wasm-loader": "^1.3.0",
		"webpack": "5.37.0",
		"webpack-cli": "4.7.2",
		"webpack-filter-warnings-plugin": "^1.2.1"
	},
	"dependencies": {},
	"keywords": [
		"cardano",
		"wallet",
		"crypto",
		"cardano-wallet",
		"cardano-wasm",
		"emurgo"
	],
	"author": "Javier Rib√≥",
	"license": "MIT",
	"private": false
}
